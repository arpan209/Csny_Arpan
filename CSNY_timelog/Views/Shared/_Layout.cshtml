    <script type="text/javascript">
        var firstAction__ = new Date();
        var count = 86400;
        var IntervalID = self.setInterval(setLastAction, 86400);

        var logoutIntervalId = "";
        function setLastAction() {
            var User_logouttime = '@Session["AdminLogOutTime"]';
           
            //var User_logouttime = 100;
            if (User_logouttime != null && User_logouttime != '0') {

                var lastAction__ = new Date();
                var secondsBetween = (lastAction__.getTime() - firstAction__.getTime()) / 1000;
                if (secondsBetween >= (User_logouttime * 60)) {

                    //             if (secondsBetween >= (1 * 60)) {

                    //alert(User_logouttime);
                    //alert('Your Session will expire in 4 minutes due to inactivity.')
                    Timeout = "";
                    //setTimeout(function () { autologout(); }, 1000);
                    count = 86400;
                    logoutIntervalId = self.setInterval(autologout, 86400);
                    
                    $('#dialog-form').dialog('open');
                    titlebar(0);
                    clearInterval(IntervalID);
                    IntervalID = "";
                }
            }
        }

        function autologout() {
            count = count - 1
            if (count > -1) {
                $("#showclock").html(count);

            }
            else {
                $("#showclock").html("");

                //$("#dialog-form").dialog("close");
                alert('We logged you out due to extended inactivity.');
                window.location = "http://net.classifiedsolutionsgroup.com/opbids/account/log_off";
                clearInterval(logoutIntervalId);
            }
        }

